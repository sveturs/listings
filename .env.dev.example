# Database
DATABASE_USER=postgres
DATABASE_PASS=password
DATABASE_DBNAME=svetu_db
DATABASE_PORT=5433

# Redis
REDIS_PORT=6380

# MinIO
MINIO_ROOT_USER=user
MINIO_ROOT_PASSWORD=password
MINIO_PORT=9002
MINIO_CONSOLE_PORT=9003
MINIO_BUCKET_NAME=listings
MINIO_LOCATION=eu-central-1

# OpenSearch
OPENSEARCH_ADMIN_PASSWORD=password
OPENSEARCH_PORT=9201
OPENSEARCH_DASHBOARD_PORT=5602
OPENSEARCH_MARKETPLACE_INDEX=marketplace

# Application Ports
BACKEND_PORT=3002
FRONTEND_PORT=3003

# API Keys (replace with real keys for full functionality)
OPENAI_API_KEY=dummy_key
GOOGLE_CLIENT_ID=dummy_key
GOOGLE_CLIENT_SECRET=dummy_key
JWT_SECRET=dummy_key
JWT_EXPIRATION_HOURS=24

# URLs
FRONTEND_URL=https://dev.svetu.rs
BACKEND_URL=https://devapi.svetu.rs
GOOGLE_OAUTH_REDIRECT_URL=https://devapi.svetu.rs/api/v1/auth/google/callback

# Frontend Public URLs
NEXT_PUBLIC_API_URL=https://devapi.svetu.rs
NEXT_PUBLIC_WEBSOCKET_URL=wss://devapi.svetu.rs
NEXT_PUBLIC_MINIO_URL=https://devs3.svetu.rs
NEXT_PUBLIC_IMAGE_HOSTS=https:devs3.svetu.rs:443,https:dev.svetu.rs:443,https:devapi.svetu.rs:443
NEXT_PUBLIC_IMAGE_PATH_PATTERN=/listings/**
NEXT_PUBLIC_ENABLE_PAYMENTS=false

# Storage
FILE_STORAGE_PROVIDER=minio
FILE_STORAGE_PUBLIC_URL=https://devs3.svetu.rs
MINIO_USE_SSL=false
NEXT_PUBLIC_GOOGLE_CLIENT_ID=dummy_key


GOOGLE_APPLICATION_CREDENTIALS=dummy_key
# Примечание: Значение GOOGLE_TRANSLATE_API_KEY должно быть корректным API ключом Google Translate
# Текущее значение имеет формат ключа OpenAI, а не Google Translate
# Для тестирования будем использовать временный пустой ключ, чтобы включить режим имитации перевода
GOOGLE_TRANSLATE_API_KEY=dummy_key
TELEGRAM_BOT_TOKEN=dummy_key
STRIPE_API_KEY=dummy_key
STRIPE_PUBLISHABLE_KEY=dummy_key
STRIPE_WEBHOOK_SECRET=dummy_key


MIGRATIONS_PATH=migrations
MIGRATION_DIRECTION=up
MIGRATION_TARGET=
MIGRATIONS_FIXTURES_PATH=fixtures
MIGRATIONS_FIXTURES_TABLE=schema_fixtures
# Migrations on API startup: off, schema, full
MIGRATIONS_ON_API=full

# JWT Configuration
DOCS_ROOT_PATH=.


MAPBOX_ACCESS_TOKEN=dummy_key
MAPBOX_SECRET_TOKEN=dummy_key
NEXT_PUBLIC_MAPBOX_TOKEN=dummy_key
NEXT_PUBLIC_MAPBOX_ACCESS_TOKEN=dummy_key
# Geocoding Configuration
GEOCODING_CACHE_TTL_HOURS=720 # 30 дней
GEOCODING_MAX_SUGGESTIONS=10
